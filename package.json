{
  "name": "miscl-toolkit-report-app",
  "productName": "MISCL Toolkit Report Generator",
  "version": "1.0.0",
  "description": "Part of the MISCL Toolkit used to generate reports from survey data",
  "main": "./lib/index.js",
  "keywords": [],
  "author": "Geoffrey E. Grimm",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && babel src --out-dir lib && bash setup.sh -ml",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.miscl-toolkit-report.app",
    "mac": {
      "category": "shiny.app",
      "asar": false
    },
    "win": {
      "asar": false
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "electron-log": "^4.0.7",
    "execa": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2"
  }
}
